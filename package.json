{
  "name": "gitfit",
  "version": "0.6.0",
  "description": "Git Feature Branch Tooling.",
  "homepage": "https://cjpatoilo.com/gitfit",
  "repository": "cjpatoilo/gitfit",
  "license": "MIT",
  "author": "CJ Patoilo <cjpatoilo@gmail.com>",
  "private": false,
  "bin": "gitfit",
  "keywords": [
    "git",
    "github",
    "opensource"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^3.8.2",
    "boeing": "latest",
    "husky": "^4.2.5",
    "inject-ga": "^0.2.4",
    "lint-staged": "^10.2.4",
    "marshmallow": "latest",
    "nyc": "^15.0.1",
    "prettier-standard": "^16.3.0",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "deploy": "marshmallow -o docs -i artwork.psd -f -m && inject-ga docs/index.html -o docs/index.html -c 'UA-24389952-13' && boeing docs && rimraf docs",
    "lint": "prettier-standard --check",
    "test": "nyc ava"
  },
  "engines": {
    "node": "^12.16.2",
    "npm": "^6.14.5"
  },
  "prettier": {
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "prettier-standard --format",
      "git add"
    ]
  }
}
